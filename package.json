{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "services/*"
  ],
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"npm run dev --workspace=apps/web\" \"npm run dev --workspace=services/auth-service\" \" npm run dev --workspace=services/message-service\"",
    "build": "vite build",
    "preview": "vite preview",
    "install:all": "npm install --workspaces"
  },
  "dependencies": {
    "@googlemaps/react-wrapper": "^1.2.0",
    "framer-motion": "^12.23.22",
    "leaflet": "^1.9.4",
    "mongoose": "^8.19.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "vite": "^4.5.14"
  }
}
